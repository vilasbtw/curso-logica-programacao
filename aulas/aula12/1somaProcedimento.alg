algoritmo "somaProcedimento"
var
valor1, valor2: inteiro
valor3, valor4: inteiro


        procedimento soma(a, b: inteiro)
        inicio
        escrevaL(a, " + ", b, " = ", a+b)
        fimProcedimento


inicio

valor1 <- 3
valor2 <- 5
soma(valor1, valor2)

valor3 <-7
valor4 <- 6
soma(valor3, valor4)

fimalgoritmo