algoritmo "mediaNumeros"
var
n1, n2: inteiro
media: real

inicio

      escreva("Digite um número: ")
                      leia(n1)
      escreva("Digite outro número: ")
                      leia(n2)
media <- (n1 + n2)/2

      escreva("A média entre ", n1, " e ", n2, " é: ", media)

fimalgoritmo
